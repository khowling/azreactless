{
  "disabled": false,
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "inputDocument",
      "type": "CosmosDB",
      "direction": "in",
      "sqlQuery": "SELECT top 10 c.id, c.name FROM c where c.active = true",
      "databaseName": "ToDoList",
      "collectionName": "Items",
      "connectionStringSetting": "MyAccount_COSMOSDB"
    },
    {
      "name": "outputDocument",
      "type": "CosmosDB",
      "direction": "out",
      "createIfNotExists": true,
      "databaseName": "ToDoList",
      "collectionName": "Items",
      "connectionStringSetting": "MyAccount_COSMOSDB"
    }
  ]
}
